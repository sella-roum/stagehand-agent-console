{
  "name": "stagehand-agent-console",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc && cd gui && pnpm install && pnpm run build",
    "start": "NODE_ENV=production node dist/src/index.js",
    "dev": "tsx src/index.ts",
    "dev:gui": "concurrently \"pnpm:dev:backend:no-cui\" \"pnpm:dev:frontend\"",
    "dev:backend": "nodemon --watch 'src/**/*.ts' --watch 'types/**/*.ts' --exec tsx src/index.ts",
    "dev:backend:no-cui": "nodemon --watch 'src/**/*.ts' --watch 'types/**/*.ts' --exec tsx src/index.ts -- --no-cui",
    "dev:frontend": "cd gui && pnpm run dev",
    "postinstall": "playwright install",
    "test:agent": "playwright test tests/agent.spec.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.23",
    "@browserbasehq/sdk": "^2.6.0",
    "@browserbasehq/stagehand": "^2.4.1",
    "@google/genai": "^1.11.0",
    "@playwright/test": "^1.49.1",
    "ai": "^4.3.19",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "http-proxy-middleware": "^3.0.0",
    "open": "^10.1.0",
    "openai": "^5.10.2",
    "ws": "^8.18.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.11",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-jsdoc": "^48.5.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0",
    "vite": "^5.3.3",
    "zustand": "^4.5.4"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
